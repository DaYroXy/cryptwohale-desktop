{
  "name": "cryptowhale",
  "private": true,
  "version": "0.0.0",
  "main": "main.js",
  "build": {
    "appId": "cryptowhale",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "icon": "electron-assets/Logo.ico"
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "electron-assets/Logo.ico",
      "uninstallerIcon": "electron-assets/Logo.ico",
      "uninstallDisplayName": "Cryptowhale Uninstallation",
      "license": "LICENSE.md",
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "main.js",
      "lib/preload.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "vite",
    "electron": "nodemon --exec electron main.js",
    "electron-dev": "concurrently \"npm start\" \" npm run electron\"",
    "electron-build": "electron-builder",
    "build": "vite build && electron-builder",
    "package": "electron-builder"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "jsencrypt": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.0",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13",
    "ws": "^8.13.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "vite": "^4.2.0",
    "wait-on": "^7.0.1"
  }
}
